{
    "defaultSeverity": "warning",
    "extends": [
        "tslint:recommended"
    ],
    "jsRules": {},
    "rules": {
        "max-classes-per-file": false,
        "interface-over-type-literal": false,
        "no-console": [
            false
        ],
        "variable-name": [true, "ban-keywords", "check-formal", "allow-leading-underscore"],
        "no-unused-variable": true,
        "no-unused-expression": true
    },
    "rulesDirectory": []
}